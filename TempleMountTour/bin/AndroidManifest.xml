<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.templemounttour"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_beit_mikdash"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.templemounttour.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.templemounttour.PrepVerifyActivity"
            android:label="@string/title_activity_prep_verify" >
        </activity>
        <activity
            android:name="com.example.templemounttour.TourMainMenuActivity"
            android:label="@string/title_activity_tour_main_menu" >
        </activity>
    </application>

</manifest>
